[color]
	diff = auto
	status = auto
	branch = auto
	grep = auto
[core]
	excludesfile = ~/.gitignore
	editor = /usr/bin/vim
[push]
	default = current
[alias]
	st = status
	co = checkout
	cb = checkout -b
	ci = commit -v
	di = diff
	dif = diff
	br = branch
	bra = branch -a
	puhs = push
	psuh = push
	pus = push
	puh = push
	pl = !git pull && git submodule update --init
	graph = log --oneline --graph --decorate=full
	push-f = push --force-with-lease
